{
  "manifest_version": 3,
  "name": "LeetCode Auto-Solver",
  "version": "1.0",
  "description": "Fetch and copy WalkCCC solution to clipboard & LeetCode editor.",
  "permissions": ["clipboardWrite", "scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"]
    }
  ]
}
